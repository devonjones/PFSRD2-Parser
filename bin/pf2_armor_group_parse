#!/usr/bin/env python
import sys
import os
from pfsrd2.armor_group import parse_armor_group
from universal.warnings import WarningReporting
from universal.options import exec_main, option_parser


def main():
    usage = "usage: %prog [options] [filenames]\nParses armor groups from pfsrd2 html to json and writes them to the specified directory"
    parser = option_parser(usage)
    (options, args) = parser.parse_args()
    options.subtype = "armor_group"
    exec_main(options, args, parse_armor_group, "armor_groups")


if __name__ == "__main__":
    sys.exit(main())
